{
    "name": "unimapper/nette",
    "type": "library",
    "description": "Official Nette extension for UniMapper",
    "keywords": ["php", "unimapper", "nette", "extension"],
    "license": ["MIT"],
    "authors": [
        {
            "name": "Bronislav Sedlak",
            "homepage": "http://github.com/bauer01"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "https://github.com/vp/unimapper.git"}
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "nette/tester": "~1.0",
        "mockery/mockery": "~0.9.0",
        "unimapper/unimapper": "dev-associations as 1.2.x-dev",
        "nette/nette": "~2.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "replace": {
        "bauer01/unimapper-nette": "self.version"
    },
    "autoload": {
         "psr-4": {"UniMapper\\Nette\\": "src/"}
    }
}